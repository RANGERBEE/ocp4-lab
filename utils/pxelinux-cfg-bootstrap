DEFAULT pxeboot
PROMPT 0
TIMEOUT 0
ONTIMEOUT pxeboot

LABEL pxeboot
  MENU DEFAULT
  KERNEL rhcos/rhcos-kernel
  APPEND ip=eno1:dhcp rd.neednet=1 initrd=rhcos/rhcos-initramfs.img console=tty0 console=ttyS0 coreos.inst=yes coreos.inst.install_dev=sda coreos.inst.ignition_url=http://192.168.1.1:8000/bootstrap.ign coreos.inst.image_url=http://192.168.1.1:8000/metal/rhcos-410.8.20190516.0-metal-bios.raw.gz